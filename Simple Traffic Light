#include <stdio.h>
#include <string.h>

int main() {
    char light[10] = "yellow";
    if (strcmp(light, "red") == 0) {
        printf("Stop.\n");
    } else if (strcmp(light, "green") == 0) {
        printf("Go.\n");
    } else {
        printf("Prepare to stop or proceed with caution.\n");
    }
    return 0;
}
